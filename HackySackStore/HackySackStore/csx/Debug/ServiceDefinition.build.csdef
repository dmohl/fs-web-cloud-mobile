<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="HackySackStore" schemaVersion="2012-05.1.7" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="HackySackStoreFrontWebApi">
    <LocalResources>
      <LocalStorage name="DiagnosticStore" sizeInMB="4096" cleanOnRoleRecycle="false" />
    </LocalResources>
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="ServiceBusIssuer" />
      <Setting name="ServiceBusKey" />
      <Setting name="ServiceBusNamespace" />
      <Setting name="ServiceBusScheme" />
      <Setting name="ServiceBusServicePath" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" localPort="80" />
    </Endpoints>
    <Certificates />
  </WebRole>
  <WorkerRole name="OrderProcessor2">
    <LocalResources>
      <LocalStorage name="DiagnosticStore" sizeInMB="4096" cleanOnRoleRecycle="false" />
    </LocalResources>
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="ServiceBusIssuer" />
      <Setting name="ServiceBusKey" />
      <Setting name="ServiceBusNamespace" />
      <Setting name="ServiceBusScheme" />
      <Setting name="ServiceBusServicePath" />
      <Setting name="TableStorageConnectionString" />
    </ConfigurationSettings>
    <Endpoints />
    <Certificates />
  </WorkerRole>
  <WorkerRole name="OrderSQLStore">
    <LocalResources>
      <LocalStorage name="DiagnosticStore" sizeInMB="4096" cleanOnRoleRecycle="false" />
    </LocalResources>
    <ConfigurationSettings>
      <Setting name="HackySackStoreConnectionString" />
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="ServiceBusIssuer" />
      <Setting name="ServiceBusKey" />
      <Setting name="ServiceBusNamespace" />
      <Setting name="ServiceBusScheme" />
      <Setting name="ServiceBusServicePath" />
    </ConfigurationSettings>
    <Endpoints />
    <Certificates />
  </WorkerRole>
</ServiceDefinition>